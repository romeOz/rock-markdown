{
    "name": "romeoz/rock-markdown",
    "description": "Markdown parser for PHP",
    "keywords": ["markdown", "markdown-extra","markup language"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-markdown/issues",
        "source": "https://github.com/romeOz/rock-markdown"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "0.10.*",
        "cebe/markdown": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "romeoz/rock-file" : "0.10.*",
        "romeoz/rock-image" : "0.10.*"
    },
    "suggest": {
      "romeoz/rock-file" : "required by thumb",
      "romeoz/rock-image" : "required by thumb"
    },
    "autoload": {
        "psr-4": {
            "rock\\markdown\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}